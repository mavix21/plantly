{
  "expo": {
    "name": "Plantly",
    "slug": "plantly",
    "scheme": "plantly",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./src/assets/icon.png",
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/caveat/Caveat_400Regular.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#9CF089",
          "image": "./src/assets/splash-image.png",
          "dark": {
            "image": "./src/assets/splash-image.png",
            "backgroundColor": "#56A082"
          },
          "imageWidth": 150
        }
      ],
      "expo-font"
    ],
    "ios": {
      "bundleIdentifier": "com.mavix21.plantly"
    }
  }
}
